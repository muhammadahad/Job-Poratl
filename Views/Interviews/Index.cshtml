@model IEnumerable<Ahad_Project.Models.Interview>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.InterviewerOneNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InterviewerThreeNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InterviewerTwoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vac)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.InterviewerOneNavigation.Contact)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InterviewerThreeNavigation.Contact)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InterviewerTwoNavigation.Contact)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vac.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
